{
  "name": "loopback-connector-cli",
  "version": "0.0.1",
  "description": "connect command line interfaces as a datasource for loopback",
  "keywords": [
    "loopback-connector-cli",
    "cli",
    "datasource",
    "loopback",
    "strongloop",
    "connector",
    "juggler",
    "command",
    "line",
    "interface"
  ],
  "homepage": "https://github.com/codejamninja/loopback-connector-cli",
  "bugs": {
    "url": "https://github.com/codejamninja/loopback-connector-cli/issues",
    "email": "jam@codejam.ninja"
  },
  "license": "MIT",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/loopback-connector-cli"
  },
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*",
    "format": "prettier --write ./src/*.ts ./src/**/*.ts",
    "lint": "npm run format && eslint ./src/**/*.ts",
    "lint:fix": "npm run format && eslint --fix ./**/*.ts",
    "prepublish": "npm run build",
    "start": "ts-node src",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@loopback/repository": "^1.2.1",
    "@types/cross-spawn": "^6.0.0",
    "@types/lodash": "^4.14.123",
    "cross-spawn": "^6.0.5",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.6",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.0",
    "eslint-config-airbnb-typescript": "^1.1.0",
    "eslint-config-jam": "^0.2.5",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "testRegex": [
      "tests/.*[^d]\\.[jt]sx?$"
    ]
  }
}
